capital-city-network:Switch_1
	rdfs:type snet:Switch ;
	enet:hasIPValue "192.168.7.200" ;	
	cptlc:hasLink capital-city-network:OvercurrentRelay_1 ;
	.

capital-city-network:DistanceRelay_1
